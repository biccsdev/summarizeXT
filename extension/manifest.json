{
    "manifest_version": 3,
    "name": "summarizext",
    "version": "0.1.0",
    "description": "Summarizes blogs/articles content and provides insights",
    "permissions": ["activeTab", "scripting", "http://localhost:3000/*", "tabs", "storage", "background"],
    "action": {
      "default_popup": "popup/index.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }